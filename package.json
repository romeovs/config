{
  "name": "config",
  "version": "1.0.0",
  "description": "Easily acces config files in a node project",
  "main": "build/index.js",
  "scripts": {
    "test": "mocha -r babelhook",
    "compile": "babel src -d build",
    "postinstall": "npm run compile",
    "test": "lab -T babel.js -S ",
