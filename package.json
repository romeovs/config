  "license": "ISC",
  "bugs": {
    "url": "https://github.com/romeovs/config/issues"
  },
  "homepage": "https://github.com/romeovs/config",
  "devDependencies": {
    "babel": "^5.0.8",
    "babel-runtime": "^5.0.11",
    "chai": "^2.2.0",
    "coveralls": "^2.11.2",
    "lab": "^5.5.1"
  },
  "dependencies": {
    "findup": "^0.1.5",
    "yamljs": "^0.2.1"
  }
